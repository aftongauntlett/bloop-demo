{
  "name": "bloopmuseum-site",
  "version": "1.0.0",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "eleventy --serve",
    "build": "eleventy",
    "dev": "eleventy --serve --watch",
    "debug": "DEBUG=Eleventy* eleventy",
    "clean": "rm -rf _site",
    "build:production": "NODE_ENV=production eleventy",
    "preview": "eleventy --serve --port=8080",
    "deploy": "npm run build && echo 'Site built! Upload the _site folder to your web host, or push to GitHub for Vercel deployment.'"
  },
  "keywords": [
    "eleventy",
    "static-site-generator",
    "museum",
    "retro-computing"
  ],
  "author": "",
  "license": "ISC",
  "description": "Bloop Computer Museum website built with Eleventy",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2"
  }
}
